{
	"options": {
		"merge-default-rules": false
	},
	"files": {
		"ignore": "**/libs/**/some-partial-name.scss"
	},
	"rules": {
		"extends-before-mixins": 0,
		"extends-before-declarations": 2,
		"placeholder-in-extend": 0,
		"mixins-before-declarations": 0,
		"one-declaration-per-line": 2,
		"empty-line-between-blocks": 2,
		"single-line-per-selector": 2,
		"no-attribute-selectors": 0,
		"no-color-hex": 0,
		"no-color-keywords": 0,
		"no-color-literals": 0,
		"no-combinators": 0,
		"no-css-comments": 1,
		"no-debug": 0,
		"no-disallowed-properties": 0,
		"no-duplicate-properties": [
			2,
			{
				"exclude": ["display", "background", "src", "outline", "background-image"]
			}
		],
		"no-empty-rulesets": 2,
		"no-extends": 0,
		"no-ids": 1,
		"no-important": 2,
		"no-invalid-hex": 2,
		"no-mergeable-selectors": 2,
		"no-misspelled-properties": 2,
		"no-qualifying-elements": [
			1,
			{
				"allow-element-with-attribute": true,
				"allow-element-with-class": true,
				"allow-element-with-id": true
			}
		],
		"no-trailing-whitespace": 2,
		"no-trailing-zero": 2,
		"no-transition-all": 0,
		"no-universal-selectors": 0,
		"no-url-domains": 0,
		"no-url-protocols": 0,
		"no-vendor-prefixes": 0,
		"no-warn": 2,
		"property-units": 0,
		"declarations-before-nesting": 2,
		"force-attribute-nesting": 0,
		"force-element-nesting": 0,
		"force-pseudo-nesting": 0,
		"class-name-format": [
			1,
			{
				"allow-leading-underscore": false,
				"convention": "hyphenatedlowercase"
			}
		],
		"function-name-format": 0,
		"id-name-format": 1,
		"mixin-name-format": 0,
		"placeholder-name-format": 2,
		"variable-name-format": 0,
		"attribute-quotes": 1,
		"bem-depth": 0,
		"border-zero": 0,
		"brace-style": 0,
		"clean-import-paths": 0,
		"empty-args": 0,
		"hex-length": 0,
		"hex-notation": 0,
		"indentation": [
			1,
			{
				"size": "tab"
			}
		],
		"leading-zero": 0,
		"max-line-length": 0,
		"max-file-line-count": 0,
		"nesting-depth": [
			1,
			{
				"max-depth": 3
			}
		],
		"property-sort-order": 0,
		"pseudo-element": 0,
		"quotes": [
			2,
			{
				"style": "double"
			}
		],
		"shorthand-values": [
			1,
			{
				"allowed-shorthands": [1, 2]
			}
		],
		"url-quotes": 1,
		"variable-for-property": 0,
		"zero-unit": 0,
		"space-after-comma": 0,
		"space-before-colon": 2,
		"space-after-colon": 2,
		"space-before-brace": 2,
		"space-before-bang": 0,
		"space-after-bang": 0,
		"space-between-parens": 0,
		"space-around-operator": 1,
		"trailing-semicolon": 2,
		"final-newline": 0
	}
}
